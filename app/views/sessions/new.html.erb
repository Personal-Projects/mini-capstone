<div id="showWell" class="well">

  <h2>Login</h2>

  <%= form_tag "/login", method: :post do %>
    <div>
    <%= label_tag :email %>
    <%= email_field_tag :email %>
    </div>
    <br>
    <div>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    </div>
    <br>
    <p><%= submit_tag "Log In" %></p>
  <% end %>

</div>
